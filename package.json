{
  "name": "colisee-api",
  "version": "1.0.0",
  "description": "Micro-service responsible for scheduling random games for SIG-Game Colisee.",
  "main": "src/index.js",
  "scripts": {
    "quick-start": "node src/index.js",
    "prestart": "npm run build",
    "start": "npm run quick-start",
    "setup": "npm install",
    "build": "tsc",
    "quick-docs": "npm run quick-docs:ts",
    "quick-docs:ts": "typedoc --tsconfig tsdoc.json --out docs/ts/",
    "quick-docs:api": "apidoc -i src/ -o docs/api/",
    "predocs": "npm run build",
    "docs": "npm run quick-docs",
    "clean:modules": "rm -rf node_modules/",
    "clean:src": "find src \\( -name *.js -o -name *.js.map \\) -exec rm {} +",
    "clean:test": "find test \\( -name *.js -o -name *.js.map \\) -exec rm {} +",
    "clean": "npm run clean:src && npm run clean:test && npm run clean:modules",
    "quick-test": "mocha test/",
    "pretest": "npm run build",
    "test": "npm run quick-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siggame/colisee-api.git"
  },
  "author": "siggame",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siggame/colisee-api/issues"
  },
  "homepage": "https://github.com/siggame/colisee-api#readme",
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/dotenv": "^2.0.20",
    "@types/express": "^4.0.35",
    "@types/knex": "0.0.40",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/request-promise": "^4.1.33",
    "apidoc": "^0.17.5",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "tsdoc": "0.0.4",
    "typedoc": "^0.5.5",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "body-parser": "^1.16.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "pg": "^6.1.2",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  }
}
